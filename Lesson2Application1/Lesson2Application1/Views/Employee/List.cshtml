@model List<Lesson2Application1.Models.Employee>
@addTagHelper *,Microsoft.AspNetCore.Mvc.TagHelpers

<body>
    <h1> Names  </h1>
    <ol>
        @foreach (var e in Model)
        {
            <li> @e.Id @e.FirstName @e.LastName <a asp-controller="Employee" asp-action="Delete" asp-route-id=@e.Id>Delete</a></li>
        }
    </ol>
</body>

